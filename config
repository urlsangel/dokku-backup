#!/usr/bin/env bash
export S3BACKUP_IMAGE=${S3BACKUP_IMAGE:="urlsangel/s3backup"}
export S3BACKUP_IMAGE_VERSION=${S3BACKUP_IMAGE_VERSION:="1.0.13"}
export S3BACKUP_ROOT=${S3BACKUP_ROOT:="/var/lib/dokku/services/s3backup"}

export PLUGIN_COMMAND_PREFIX="s3backup"
export PLUGIN_DATA_ROOT=$S3BACKUP_ROOT
export PLUGIN_DEFAULT_ALIAS="S3BACKUP"
export PLUGIN_ALT_ALIAS="DOKKU_S3BACKUP"
export PLUGIN_IMAGE=$S3BACKUP_IMAGE
export PLUGIN_IMAGE_VERSION=$S3BACKUP_IMAGE_VERSION
export PLUGIN_SCHEME="s3backup"
export PLUGIN_SERVICE="S3Backup"
